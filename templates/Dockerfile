#
# Do not change content here, image automatically built
#
FROM ${BUILD_DOCKERFILE_IMAGE}

${DOCKERFILE_BUILDER_ENVVARS}

${DOCKERFILE_BUILDER_IMPORTS}

RUN chmod +rx ${DOCKERFILE_BUILDER_PATHS_DOCKERFILE_SETUP} && \
    chmod +rx ${DOCKERFILE_BUILDER_PATHS_DOCKERFILE_CONFIG} && \
    sync && \
    #. ${DOCKERFILE_BUILDER_PATHS_DOCKERFILE_ENV} && \
    ${DOCKERFILE_BUILDER_PATHS_DOCKERFILE_SETUP} 

EXPOSE ${BUILD_DOCKERFILE_PORTS_MAIN} ${BUILD_DOCKERFILE_PORTS_ADDITIONAL}

ENTRYPOINT ["/bin/sh", "-c"]
${DOCKERFILE_BUILDER_CMD}